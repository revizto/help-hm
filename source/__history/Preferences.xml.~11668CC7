    <para styleclass="Note"><text styleclass="Note" translate="true">N</text><text styleclass="Note" translate="true">o</text><text styleclass="Note" translate="true">t</text><text styleclass="Note" translate="true">e</text><text styleclass="Note" translate="true">:</text><text styleclass="Note" translate="true">&#32;</text><var styleclass="Note">&lt;%BRAND_NAME%&gt;</var><text styleclass="Note" translate="true">&#32;</text><text styleclass="Note" translate="true">a</text><text styleclass="Note" translate="true">l</text><text styleclass="Note" translate="true">s</text><text styleclass="Note" translate="true">o</text><text styleclass="Note" translate="true">&#32;</text><text styleclass="Note" translate="true">s</text><text styleclass="Note" translate="true">u</text><text styleclass="Note" translate="true">p</text><text styleclass="Note" translate="true">p</text><text styleclass="Note" translate="true">o</text><text styleclass="Note" translate="true">r</text><text styleclass="Note" translate="true">t</text><text styleclass="Note" translate="true">s</text><text styleclass="Note" translate="true">&#32;</text><text styleclass="Note" translate="true">3</text><text styleclass="Note" translate="true">D</text><text styleclass="Note" translate="true">C</text><text styleclass="Note" translate="true">o</text><text styleclass="Note" translate="true">n</text><text styleclass="Note" translate="true">n</text><text styleclass="Note" translate="true">e</text><text styleclass="Note" translate="true">x</text><text styleclass="Note" translate="true">i</text><text styleclass="Note" translate="true">o</text><text styleclass="Note" translate="true">n</text><text styleclass="Note" translate="true">&#32;</text><text styleclass="Note" translate="true">d</text><text styleclass="Note" translate="true">e</text><text styleclass="Note" translate="true">v</text><text styleclass="Note" translate="true">i</text><text styleclass="Note" translate="true">c</text><text styleclass="Note" translate="true">e</text><text styleclass="Note" translate="true">s</text><text styleclass="Note" translate="true">&#32;</text><text styleclass="Note" translate="true">f</text><text styleclass="Note" translate="true">o</text><text styleclass="Note" translate="true">r</text><text styleclass="Note" translate="true">&#32;</text><text styleclass="Note" translate="true">n</text><text styleclass="Note" translate="true">a</text><text styleclass="Note" translate="true">v</text><text styleclass="Note" translate="true">i</text><text styleclass="Note" translate="true">g</text><text styleclass="Note" translate="true">a</text><text styleclass="Note" translate="true">t</text><text styleclass="Note" translate="true">i</text><text styleclass="Note" translate="true">o</text><text styleclass="Note" translate="true">n</text><text styleclass="Note" translate="true">.</text></para>
    <para styleclass="Heading 3"><text styleclass="Heading 3" translate="true">Game mode</text></para>
    <para styleclass="Normal"><text styleclass="Normal" translate="true">Note that for the </text><text styleclass="GUI_item" translate="true">Game</text><text styleclass="Normal" translate="true"> mode additional mouse settings are available.</text></para>
    <para styleclass="Normal" style="text-align:center;"><image src="preferences_navigation_game.png" scale="60.00%" width="740" height="466" styleclass="Image Caption"></image></para>
    <para styleclass="Normal"><table styleclass="Default2" rowcount="7" colcount="2">
      <tr style="vertical-align:top">
